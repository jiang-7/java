public  List<Integer> preorderTraversal(TreeNode root){
        //新建一个链表用于存储遍历结果
       List<Integer> list=new ArrayList<>(  );
       if (root == null){
         return list;
       }
       NLRHelp(root,list);
       return list;
   }
    public  void  NLRHelp(TreeNode root ,List<Integer> list){
        if(root==null){
            return;
        }else {
            list.add( root.val );
            NLRHelp( root.left, list );
            NLRHelp( root.right, list );
        }
   }
